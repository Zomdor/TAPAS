#  DEPENDS system_lib
)
include_directories(
  ${catkin_INCLUDE_DIRS}
)

include_directories(include ${catkin_INCLUDE_DIRS})

add_executable(Gps main.cpp)
target_link_libraries(Gps ${catkin_LIBRARIES})
