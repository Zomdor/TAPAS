cmake_minimum_required(VERSION 2.8.3)
project(Gps)
find_package(catkin REQUIRED COMPONENTS
  roscpp
  rospy
  std_msgs
  message_generation
)

add_message_files(
   FILES
   Num.msg
 )

## Generate services in the 'srv' folder
add_service_files(
   FILES
   AddTwoInts.srv
   SubtractInts.srv
 )

generate_messages(
   DEPENDENCIES
   std_msgs
 )

catkin_package(
#  INCLUDE_DIRS include
